<h3>End Users Details </h3>
<% if flash[:success] %>
  <font color="green">
    <%= flash[:success] %><% flash.clear %>
  </font>
<% end %>
<table>
  <tr>
    <th> First Name </th>
    <th> Last Name </th>
    <th> Email </th>
    <th> Mobile Number </th>
    <th> Policy Number </th>
    <th> Damage Details </th>
  </tr>
  <% @end_users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.mobile %></td>
      <td><%= user.policy_number %></td>
      <td><%= user.damage_details %></td>
    </tr>
  <% end %>
</table>